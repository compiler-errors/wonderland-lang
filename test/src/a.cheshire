fn main() -> Int {
    let x = [1, 2, 3, 4, 5, 4, 3, 2, 1, -1].
    let i = 0.
    let s = "[".

    while i < 10 {
        if i > 0 {
            s = s + ", ".
        }

        s = s + x[i]:into().
        i = i + 1.
    }

    s = s + ']':into().
    print(s + '\n':into()).

    print("> " + "hello"[2]:into() + "\n").

    0
}