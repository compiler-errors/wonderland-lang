impl<_T> Into<String> for [_T] where _T: Into<String> {
    fn into(self) -> String {
        let i = 0.
        let s = "[".

        while i < self:len() {
            if i > 0 {
                s = s + ", ".
            }

            s = s + self[i]:into().
            i = i + 1.
        }

        s = s + ']':into().
        s
    }
}

fn main() -> Int {
    let x = [1, 2, 3, 4, 5, 4, 3, 2, 1, -1].

    print("Array len is: " + x:len():into() + "\n").
    print(x:into() + '\n':into()).
    0
}